# B. Regex Power

grep -E '[0-9]{3}' file.txt
grep -E '([0-9]{1,3}\.){3}[0-9]{1,3}' file.txt
grep -E '[0-9]{4}-[0-9]{2}-[0-9]{2}' file.txt
grep -E '[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,}' file.txt
grep -E '[0-9]' file.txt
grep -E '^[0-9]+$' file.txt
grep -E '^#' file.txt
grep -E '^.{20,}$' file.txt
grep -E 'foo' file.txt | grep -Ev 'bar'
grep -E '([[:alpha:]]+).*\1' file.txt
grep -E '\b([[:alpha:]]+)\b.*\b\1\b' file.txt
